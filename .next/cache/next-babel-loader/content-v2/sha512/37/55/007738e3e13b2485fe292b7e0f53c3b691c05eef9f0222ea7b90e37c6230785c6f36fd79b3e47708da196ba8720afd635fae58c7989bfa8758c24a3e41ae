\n      email,\n      password\n    }\n\n    // Check if each field was filled\n    if (user.name === '') {\n      console.log('name empty')\n      setErrors({ ...errors, name: 'You must enter your name.' })\n    } else {\n      console.log('name not empty')\n      setErrors({ ...errors, name: '' })\n    }\n\n    if (user.email === '') {\n      setErrors({ ...errors, email: 'You must enter a valid email.' })\n    } else {\n      setErrors({ ...errors, email: '' })\n    }\n\n    if (user.password === '') {\n      setErrors({ ...errors, password: 'You must enter a password.' })\n    } else {\n      setErrors({ ...errors, password: '' })\n    }\n\n    console.log(errors)\n    // usersRef.push(user)\n  }\n  return (\n    <form onSubmit={submitRegister}>\n      <input\n        placeholder='Name'\n        onChange={(e) => setName(e.target.value)}\n        required\n      />\n      <Error>{errors.name}</Error>\n      <input\n        placeholder='Email'\n        type='email'\n        onChange={(e) => setEmail(e.target.value)}\n        required\n      />\n      <Error>{errors.email}</Error>\n      <input\n        placeholder='Password'\n        type='password'\n        onChange={(e) => setPassword(e.target.value)}\n        required\n      />\n      <Error>{errors.password}</Error>\n      <button type='submit'>Submit</button>\n    </form>\n  )\n}\n\nexport default Register\n"]},"metadata":{},"sourceType":"module"}